function
v0 = $1 + $1
ret v0
end

function
v0 = p0 + p0
ret v0
end

function
v0 = p0 + $1
ret v0
end

function
p0 = $1 + $1
ret p0
end

function
p0 = p0 + p0
ret p0
end

function
p0 = $1 + p0 
ret p0
end

function
v0 = $-1
p0 = $1 + v0 
ret p0
end

function
v0 = $-2
v0 = $1 + v0 
ret v0
end

function (nao testei) (fiz sem pensar se entra ou n em loop inf)
v0 = p0*p0
p0 = p0 - $1
zret v0 p0
v1 = v0 * v0
v2 = call 0 p0
p0 = p0 - $1
zret v1 p0
ret v2
end
